% Please edit documentation in R/segment.R
\name{calculate_CNV_arm_level}
\alias{calculate_CNV_arm_level}
\title{Arm-level CNV}
\usage{
calculate_CNV_arm_level(
  norm_count,
  baseline,
  genome = "hg38",
  min_interval = 5,
  smooth = F,
  output_dir = "./",
  output_name = "CNV_result.rds"
)
}
\arguments{
\item{norm_count}{a normalized read count matrix. Obtained from previous step
[normalize()].}

\item{baseline}{read count baseline of normal cells. Obtained from previous step
[normalize()].}

\item{genome}{reference genome, "hg19" or "hg38".}

\item{min_interval}{minimum bin number between breakpoints.}

\item{smooth}{logical. Whether to aggregate similar cells to smooth final CNV
results?}

\item{output_dir}{output directory}

\item{output_name}{output file name, should be ".rds"}

\item{lambda}{parameter of BICseq2 algorithm that controls the overall
resolution of segmentation result. In general, more breakpoints will be reported
when smaller [lambda] is used.}
}
\value{
a list of the following objects.
* "CNV": a cell-by-bin copy ratio matrix.
* "bkp": breakpoint positions
* "CNV_seg": a cell-by-segment copy ratio matrix.
}
\description{
Arm-level CNV
}
\examples{

}
